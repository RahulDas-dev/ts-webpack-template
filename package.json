{
  "name": "webpack_ts_template",
  "version": "1.0.0",
  "description": "WebPack Typescript template ",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ParoRahul/ts-webpack-template.git"
  },
  "homepage": ".",
  "keywords": [
    "webpack",
    "Typescript"
  ],
  "engines": {
    "node": ">= 10",
    "npm": ">= 1.6"
  },
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack serve --open",
    "build": "cross-env NODE_ENV=production webpack ",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\"  mocha -r ts-node/register test/**/*.test.ts",
    "test:report": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\"  mocha --reporter mochawesome -r ts-node/register test/**/*.test.ts",
    "version": "webpack --version",
    "lint": "eslint  --ext .ts .",
    "lint:fix": "eslint --ext .ts  --fix ."
  },
  "author": "Rahul Das",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@svgr/webpack": "^5.5.0",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-webpack-plugin": "^3.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "interpolate-html-plugin": "^4.0.0",
    "mini-css-extract-plugin": "^2.3.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.8",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.1.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^6.7.0",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "webpack": "^5.56.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0",
    "webpack-manifest-plugin": "^4.0.2"
  },
  "dependencies": {
    "bootstrap": "^5.1.1",
    "bootstrap-icons": "^1.5.0"
  }
}
